{
	"name": "demo",
	"version": "10.5.0",
	"private": true,
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"deploy": "npm run build && node --import tsx scripts/deploy.ts",
